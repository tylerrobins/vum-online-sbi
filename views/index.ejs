<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>VUM - Index's</title>
</head>

<body>
    <h2> Avaliable Channels</h2>
    <% Object.entries(availableChannels).forEach(([key, value]) => { %>
        <h3> - <a href="<%=value.route%>/"><%= value.title %></a></h3>
    <% }) %>
    <h2>Routes</h2>
    <% Object.entries(avaliableRoutes).forEach(([key, value]) => { %>
        <h4>  <%= value.title %></h4>
        <ul>
            <li>Method: <%= value.method %></li>
            <li>Description: <%= value.description %></li>
            <% if (value.route != '') { %>
            <li>Route: <a href="<%= value.route %>"><%= value.route %></a></li>
            <% } %>
        </ul>
    <% }) %>
</body>

</html>